pipeline {
    agent any

    stages {
        stage('Example') {
            steps {
                script {
                    // Your Git-related steps here

                    // Assuming you want to run Docker commands within a Docker container
                    node {
                        checkout scm

                        // Your Docker-related steps here
                        docker.image('liquibase/liquibase').run {
                            // Your steps inside the Docker container
                            sh 'echo "Hello from Docker"'
                        }
                    }
                }
            }
        }
    }
}
