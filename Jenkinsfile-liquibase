pipeline {
  agent {
    docker { image 'liquibase/liquibase:4.4.2' }
  }
  stages {
    stage('Status') {
      steps {
        sh 'liquibase status --url="jdbc:mysql://34.93.119.220:3306/test1" --changeLogFile=file.xml --username=root'
      }
    }
    stage('Update') {
      steps {
        sh 'liquibase update --url="jdbc:mysql://34.93.119.220:3306/test1" --changeLogFile=file.xml --username=root'
      }
    }
  }
  post {
    always {
      cleanWs()
    }
  }
}
